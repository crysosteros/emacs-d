(setq warp-dir (concat dotfiles-dir "tops/"))
(setq warp-table (make-hash-table :test 'equal))

(defun new-warp (name place)
  (let ((warp (concat warp-dir place)))
    (make-directory warp 't)
    (puthash name warp warp-table)))

